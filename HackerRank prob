int beautifulDays(int i, int j, int k) {
    int bd=0,l,t,news,r,count;
    for(l=i;l<=j;l++)
    {
        t=l;    count=0;
        while(t)
        {
            t=t/10;
            count++;
        }
        t=l;    news=0; 
        for(j=0;j<count;j++)
        {
            t=l/pow(10,j);
            r=t%10;
            news+=r*pow(10,count-j-1);
        }
        if((abs(news-l))%k==0)
            bd++;
    }   
    return bd;
