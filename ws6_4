#include<stdio.h>


int main()
{ int n; 
    printf("Enter size\n");
    scanf("%d",&n);
    printf("enter input\n");
    int a[n];int flag=0,i;
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);printf(" ");
    }
    printf("\n");
    for(i=0;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(a[i]>a[j])
            {
                flag=1;  break;
            }
        if(flag)
        {
         break;   
        }
        }
    }
        if(flag)
        {
            printf("unsorted");
        }
        else
        {
            printf("sorted");
        }
            
        
    return 0;
}
